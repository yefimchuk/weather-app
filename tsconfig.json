{
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "module": "*.png",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "dist",
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx"
  },
  "exclude": [
    "dist",
    "node_modules"
  ],
  "extends": "expo/tsconfig.base",
  "typeRoots": ["./src/types"]
}
